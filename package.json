{
    "name": "vtexio-snippets-plugin",
    "displayName": "vtexio snippets plugin",
    "description": "c",
    "publisher": "1marlonpassos",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Snippets"
    ],
    "license": "GPL-3.0",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/MarlonPassos-git/vtexio-snippets-plugin.git"
    },
    "contributes": {
        "snippets": [
            {
                "language": "json",
                "path": "dist/snippets/snippets.json.code-snippets"
            },
            {
                "language": "jsonc",
                "path": "dist/snippets/snippets.json.code-snippets"
            },
            {
                "language": "scss",
                "path": "dist/snippets/snippets.css.code-snippets"
            },
            {
                "language": "css",
                "path": "dist/snippets/snippets.css.code-snippets"
            }
        ],
        "jsonValidation": [
            {
                "fileMatch": "src/*.json",
                "url": "./src/schema/schema.json"
            },
            {
                "fileMatch": "src/*.json",
                "url": "./src/schema/schema.json"
            }
        ]
    },
    "scripts": {
        "dev": "gulp dev"
    },
    "dependencies": {
        "global": "^4.4.0",
        "typescript-json-schema": "^0.53.0"
    },
    "devDependencies": {
        "gulp": "^4.0.2",
        "gulp-merge-json": "^2.1.2"
    }
}
