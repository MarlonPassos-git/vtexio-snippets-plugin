{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "block title",
	"properties": {
		"store-icons": {
			"type": "object",
      "additionalProperties": false,
			"markdownDescription": "https://developers.vtex.com/vtex-developer-docs/docs/vtex-store-icons\n\n`store-icons` All Store icons components.\n\n`\"vtex.store-icons\": \"0.x\"`",
			"properties": {
				"blockClass": {
					"type": "string",
					"description": "Class Name"
				},
				"title": {
					"type": "string",
					"description": "Title showed in site editor"
				},
				"props": {
					"type": "object",
					"description": "Block's properties",
					"properties": {
						"id": {
							"type": "string",
							"description": "The ID for the desired icon"
						},
						"size": {
							"type": "number",
							"description": "Desired size",
							"default": 16
						},
						"isActive": {
							"type": "boolean",
							"description": "desc",
							"default": true
						},
						"activeClassName": {
							"type": "string",
							"description": "The className it should have if active"
						},
						"mutedClassName": {
							"type": "string",
							"description": "The className it should have if not active"
						},
						"type": {
							"type": "string",
							"enum": ["filled", "line", "outline"]
						},
						"orientation": {
							"type": "string",
							"enum": ["up", "down", "left", "right"]
						},
						"state": {
							"type": "string",
							"enum": ["on", "off"]
						},
						"shape": {
							"type": "string",
							"enum": ["square", "rounded", "circle"]
						}
					}
				}
			}
		}
	}
}
